@model List<Disk>
@{
    ViewBag.Title = "Disk Manager";
}

<h1>@ViewBag.Title</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Disk ID</th>
            <th>Release Date</th>
            <th>CD Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var disks in Model)
        {
            <tr>
                <td>@disks.DiskId</td>
                <td>@disks.ReleaseDate</td>
                <td>@disks.CdName</td>
            </tr>
        }
    </tbody>
</table>